{
  "name": "@omni-kit/builder-core",
  "version": "0.0.0",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "bin": {
    "omni-builder": "dist/cli.mjs",
    "omni": "dist/cli.mjs",
    "ob": "dist/cli.mjs"
